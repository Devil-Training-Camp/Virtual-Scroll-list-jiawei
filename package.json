{
	"name": "vue3-virtual-list",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "husky install",
		"lint:eslint": "eslint \"{src,mock,build}/**/*.{vue,js,ts,tsx}\" --fix",
		"format": "prettier --write \"./**/*.{html,vue,ts,js,jsx}\"",
		"lint:prettier": "prettier --write  \"src/**/*.{js,ts,jsx,tsx,css,less,scss,vue,html}\"",
		"lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss,html}\"",
		"lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.cjs",
		"commit": "git-cz"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-git"
		}
	},
	"dependencies": {
		"vue": "^3.2.47"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.6",
		"@commitlint/config-conventional": "^17.6.6",
		"@vitejs/plugin-vue": "^4.1.0",
		"commitizen": "^4.3.0",
		"cz-git": "^1.6.1",
		"eslint": "^8.43.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-config-standard": "^17.1.0",
		"eslint-config-xo": "^0.43.1",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-n": "^16.0.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-vue": "^9.15.1",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.2",
		"postcss": "^8.4.24",
		"postcss-html": "^1.5.0",
		"prettier": "^2.8.8",
		"stylelint": "^14.0.0-0",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-prettier": "^9.0.5",
		"stylelint-config-recess-order": "^3.1.0",
		"stylelint-config-recommended-vue": "^1.4.0",
		"stylelint-config-standard": "^23.0.0-0",
		"stylelint-config-standard-scss": "^2.0.0-0",
		"vite": "^4.3.9",
		"vue-eslint-parser": "^9.3.1"
	}
}
